[push]
	default = current
[core]
	editor = /usr/bin/vim
[color]
	ui = true
[user]
	name = syguer
	email = ksk.i.530@gmail.com
[alias]
  st = status
  aa = add .
  co = checkout
  cob = checkout -b
  gr = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
  ct = commit
  ctm = commit -m
  cta = commit --amend
  rb = rebase
  br = branch
  bra = branch -a
  cpk = cherry-pick
  po = push origin
